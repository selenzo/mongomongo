{
  "name": "mongomongo",
  "version": "1.0.0",
  "description": "REST API",
  "main": "server.js",
  "scripts": {
    "db": "start mongod --dbpath data --journal --storageEngine=mmapv1",
    "db-close": "start mongo admin --eval \"db.shutdownServer();\"",
    "pretest": "start mongod --dbpath data --journal --storageEngine=mmapv1",
    "postinstall": "start mongo admin --eval \"use mongomongo; db.createUser({user: \"mongomongo\",pwd: \"test\",roles: [{role: \"readWrite\",db: \"mongomongo\"}]})\"",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selenzo/mongomongo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/selenzo/mongomongo/issues"
  },
  "homepage": "https://github.com/selenzo/mongomongo#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "config-yml": "^0.6.1",
    "connect-timeout": "^1.8.0",
    "express": "^4.15.2",
    "mongodb": "^2.2.24",
    "monk": "^4.0.0",
    "node-datetime": "^1.0.0"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-mocha": "^4.1.0",
    "gulp-nodemon": "^2.2.1",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "supertest": "^3.0.0"
  }
}
